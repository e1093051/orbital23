{
  "expo": {
    "name": "orbital2023",
    "slug": "orbital2023",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        ,
          "cameraPermission":  "The app accesses your camera to let you share ph with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
      "NSCameraUsageDescription": "Allow access to the camera",
      "NSPhotoLibraryUsageDescription": "Allow access to the photo library",
      "NSMicrophoneUsageDescription": "Allow access to the microphone (optional)"
    },
      "supportsTablet": true,
      "bundleIdentifier": "com.hiteshri.orbital2023"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ],
      "package": "com.hiteshri.orbital2023"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "357a4276-6628-4f6a-8761-c24c0586642e"
      }
    }
  }
}
